{
    "$schema": "./node_modules/ng-swagger-gen/ng-swagger-gen-schema.json",
    "swagger": "https://api.lightest.xyz/swagger/v1/swagger.json",
    "output": "src/app/api",
    "ignoreUnusedModels": true,
    "minParamsForContainer": 2,
    "sortParams": "none",
    "defaultTag": "Api",
    "removeStaleFiles": true,
    "modelIndex": true,
    "serviceIndex": true,
    "apiModule": true,
    "enumModule": true,
    "generateExamples": false
}